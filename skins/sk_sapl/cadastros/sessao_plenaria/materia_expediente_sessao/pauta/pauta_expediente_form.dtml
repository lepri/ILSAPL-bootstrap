function reloadParentAndClose()
{
    // reload the opener or the parent window
    window.opener.location.reload();
    // then close this pop-up window
    window.close();
} 

</SCRIPT>

<table class="table">
  <tr><td>

<div class="helpsapl"> <a id="ajuda" href="&dtml-portal_url;/ajuda/pauta_expediente"><span class="glyphicon glyphicon-hand-right"></span> Ajuda</a> </div>
<h1 class="firstHeading"> Matérias do Expediente </h1>

<h3 class="center"><dtml-var sessao_plenaria_header></h3>

<h3>Gerar Pauta do Expediente</h3>

   <table width="100%" class="center">

     <form name="pauta_expediente_form" method="post" action="pauta_expediente_gerar_proc">

	  <tr>
	     <th colspan="2">
		<label for="lst_status">Selecione a Situa&ccedil;&atilde;o Atual das Mat&eacute;rias</label><span class="campoObrigatorio">(Obrigatório)</span>
	     </th>
	  </tr>

	  <tr>
       <td>
          <select id="lst_status" name="lst_status">
            <option value=""></option>
            <dtml-in expr="zsql.status_tramitacao_obter_zsql(ind_excluido=0)">
              <option value="<dtml-var cod_status missing>">
                <dtml-var sgl_status> - <dtml-var des_status>
              </option>
            </dtml-in>
          </select>
        </td>
          </tr>

          <tr>
           <td class="center">
            <input type="button" name="btn_pauta_expediente_form" class="btn btn-primary" value="Continuar"
                   onClick="campos_criticar(document.pauta_expediente_form)">&nbsp;&nbsp;
            <input type="button" class="btn btn-primary" name="btn_fechar" value="Fechar" OnClick="reloadParentAndClose();" />
            <input type="hidden" name="cod_sessao_plen" value="<dtml-var cod_sessao_plen missing>" />
            <input type="hidden" name="cod_sessao_leg" value="<dtml-var cod_sessao_leg missing>" />
            <input type="hidden" name="cod_num_legislatura" value="<dtml-var cod_num_legislatura missing>" />
            <input type="hidden" name="tip_sessao" value="<dtml-var tip_sessao missing>" />
            <dtml-in expr="zsql.sessao_plenaria_obter_zsql(cod_sessao_plen = cod_sessao_plen, ind_excluido=0)">
                <input type="hidden" name="dat_sessao" value="<dtml-var dat_inicio_sessao>" />
                <input type="hidden" name="dat_ordem" value="<dtml-var dat_inicio_sessao>" />
            </dtml-in>
           </td>
          </tr>

     </form>
   </table>


</td></tr>
</table>
