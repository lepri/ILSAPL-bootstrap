<!DOCTYPE html>
<html lang="pt-br">
<dtml-if expr="_.has_key('QUERY_STRING') and QUERY_STRING=='retry=1' and portal_membership.isAnonymousUser()">
    <dtml-call expr="REQUEST['RESPONSE'].expireCookie('__ac', path='/')">
    <dtml-call expr="REQUEST['RESPONSE'].redirect(portal_url() +'/index_html?retry=0')">
</dtml-if>
<dtml-call "RESPONSE.setHeader('Content-Type','text/html; charset=utf-8')">
<dtml-call "RESPONSE.setHeader('Expires', 'Sat, 1 Jan 2000 00:00:00 GMT')">
<dtml-call "RESPONSE.setHeader('Cache-Control', 'private, no-cache, no-store, must-revalidate, post-check=0, pre-check=0')">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="author" content="Interlegis">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.6, minimum-scale=0.4">
    <link rel="shortcut icon" type="image/x-icon" href="&dtml-portal_url;/imagens/favicon.ico" />
    <title>Sistema de Apoio ao Processo Legislativo</title>
    <meta name="generator" content="SAPL - Sistema de Apoio ao Processo Legislativo - http://www.interlegis.leg.br/sapl" />
    <dtml-var standard_css_slot>
    <script type="text/javascript" src="&dtml-portal_url;/tema/js/jquery.min.js"></script>
    <script type='text/javascript' src='&dtml-portal_url;/tema/js/bootstrap.min.js'></script>
    <script type='text/javascript' src='&dtml-portal_url;/tema/js/jquery.cookie.js'></script>
    <script type='text/javascript' src='&dtml-portal_url;/tema/js/bootstrapValidator.min.js'></script>
    <script type="text/javascript" src='&dtml-portal_url;/tema/js/bootstrap-datepicker.js'></script>
    <script type="text/javascript" src='&dtml-portal_url;/tema/js/bootstrap-colorpicker.min.js'></script>
    <script type="text/javascript" src='&dtml-portal_url;/tema/js/locales/bootstrap-datepicker.pt-BR.js'></script>
    <script type="text/javascript" src='&dtml-portal_url;/tema/js/language/pt_BR.js'></script>
    <script type='text/javascript' src='&dtml-portal_url;/tema/js/geral.js'></script>
    <script type='text/javascript' src='&dtml-portal_url;/tema/js/datas.js'></script>
</head>
<body class="container-fluid">
    <dtml-if expr="_.has_key('impressao')">
        <dtml-var printable_html_top_bar>
    <dtml-elif expr="_.has_key('iframe') or REQUEST.SESSION.get('iframe')">
        <dtml-call expr="REQUEST.SESSION.set('iframe', 1)">
    <dtml-else>
        <dtml-var standard_html_superior>
        <dtml-var standard_html_top_bar>

        <div class="container">



    </dtml-if>
<dtml-if expr="REQUEST.has_key('portal_status_message')">
  <p id="system-message"><dtml-var portal_status_message></p>
</dtml-if>

